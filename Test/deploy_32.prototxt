layer {
  name: "score59-a"
  type: "Convolution"
  bottom: "fc7"
  top: "score59-a"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 18
    kernel_size: 1
    engine: CAFFE
    weight_filler {
	  type: "xavier"
	  std: 0.1
	}
	bias_filler {
	  type: "constant"
	  value: 0.2
	}
  }
}
layer {
  name: "upscore-a"
  type: "Deconvolution"
  bottom: "score59-a"
  top: "upscore-a"
  param {
    lr_mult: 0
    decay_mult: 0
  }
  convolution_param {
    num_output: 18
    group:18
    bias_term: false
    kernel_size: 64
    stride: 32
    pad:4
    weight_filler: { type: "bilinear" }
  }
}
layer { type: 'Crop' name: 'score' top: 'score-a'
  bottom: 'upscore-a' bottom: 'data' crop_param {
    axis: 2
    offset: 19
} }



